{
  "name": "material-ui-vite-ts",
  "private": true,
  "version": "5.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "yarn clean && cross-env NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "lint": "yarn lint:types && yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "lint:types": "yarn tsc --noemit",
    "lint:eslint": "yarn eslint .",
    "lint:eslint:fix": "yarn eslint . --fix",
    "lint:prettier": "yarn prettier . -c",
    "lint:prettier:fix": "yarn prettier . -w"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.10.5",
    "@emotion/is-prop-valid": "^1.2.0",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@react-hook/throttle": "^2.2.0",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.47.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "simplebar-react": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "latest",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.0",
    "typescript": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
